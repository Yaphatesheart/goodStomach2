<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
			
<mapper namespace="com.goodStomach.dao.UserMapper">
	
	<insert id="insertOne" parameterType="com.goodStomach.entity.User">
		insert into tbl_user(id,username,password,phone,address) values(#{id},#{username},#{password},#{phone},#{address})
	</insert>
	
	<select id="selectUserByName" parameterType="java.lang.String" resultType="com.goodStomach.entity.User">
		select * from tbl_user where username=#{username}
	</select>
	
	<select id="selectAll" resultType="com.goodStomach.entity.User">
		select * from tbl_user
	</select>
	
	<delete id="deleteById" parameterType="java.lang.Long">
		delete from tbl_user where id=#{id}
	</delete>
	
</mapper>